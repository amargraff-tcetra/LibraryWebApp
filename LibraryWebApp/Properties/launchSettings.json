      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5237"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7266;http://localhost:5237"
